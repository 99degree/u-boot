{
	/* When running as a first-stage bootloader or with embedded
	 * DT we can't rely on this being populated for us.
	 */
	memory@80000000 {
		device_type = "memory";
		reg = <0 0x80000000 0 0xfdfa0000>;
	};
};

/* This GPIO must be turned on to enable the 5v VBUS
 * supply on the USB port.
 */
&usb_2_dwc3 {
	vbus-supply = <&pcie0_3p3v_dual>;
};
